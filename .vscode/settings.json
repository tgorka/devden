{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/dist": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "addgroup",
    "adduser",
    "autopep",
    "autoremove",
    "bierner",
    "chsh",
    "dbaeumer",
    "debugpy",
    "devcontainers",
    "devden",
    "donjayamanne",
    "dpkg",
    "eamodio",
    "errorlens",
    "esbenp",
    "githistory",
    "githubcli",
    "htmlcov",
    "intellicode",
    "jerrygoyal",
    "keyrings",
    "kohler",
    "libbz",
    "libncurses",
    "libncursesw",
    "libreadline",
    "libsqlite",
    "linuxbrew",
    "miniconda",
    "mypy",
    "njsproj",
    "noninteractive",
    "NOPASSWD",
    "ntvs",
    "OHMYZSH",
    "procps",
    "pycache",
    "Pylance",
    "pylint",
    "pytest",
    "remotehub",
    "rustup",
    "setuptools",
    "shellenv",
    "technote",
    "tgorka",
    "tzdata",
    "usernamehw",
    "visualstudioexptteam",
    "vscodeintellicode",
    "yzhang"
  ]
}
